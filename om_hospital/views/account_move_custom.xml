<?xml version="1.0" ?>
<odoo>
    <data>
    <record id="view_hospital_inherit_account_move" model="ir.ui.view">
        <field name="name">view.hospital.account.move.custom</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='other_info']" position="after">
                <page name="custom" string="Custom Invoice ">
                    <field name="invoice_line_customs_ids">
                        <tree editable="bottom">
                            <field name="product_id"/>
                            <field name="name"/>
                            <field name="quantity"/>
                        </tree>
                    </field>
                </page>
            </xpath>
        </field>
    </record>
</data>
</odoo>